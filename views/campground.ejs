    <% layout('layouts/boilplate') %>
    <h2>All campgrounds</h2> 
    <% for(let camp of camps) { %>
        <div class="card mt-4 mb-4">
         <div class="row pt-4 pb-4">
            <div class="col-md-4">
                <img class="img-fluid" src="<%= camp.image %>" alt="image not found" >
            </div>
            <div class="col-md-8">
                <h5 class="card-title"><%= camp.title %></h5>
                <p class="card-text"><%= camp.descreption %></p>
                <p class="card-text">
                    <small class="text-muted"><%= camp.location %></small>
                </p>
                <a class="btn btn-primary" href="/campground/<%= camp.id %>" >view <%= camp.title %></a>
            </div>
         </div>
        </div>
    <%}%>
  